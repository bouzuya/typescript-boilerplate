{
  "name": "typescript-boilerplate",
  "description": "typescript-boilerplate",
  "version": "0.1.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/typescript-boilerplate/issues"
  },
  "devDependencies": {
    "@types/node": "6.0.38",
    "@types/power-assert": "0.0.27",
    "@types/proxyquire": "1.3.26",
    "@types/sinon": "1.16.29",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "espower-babel": "^4.0.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^2.3.4",
    "power-assert": "^1.2.0",
    "proxyquire": "1.7.10",
    "sinon": "1.17.5",
    "typescript": "2.0.0"
  },
  "homepage": "https://github.com/bouzuya/typescript-boilerplate#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/typescript-boilerplate"
  },
  "scripts": {
    "build": "npm run build-ts && npm run build-es2015 && npm run copy-to-lib",
    "build-es2015": "babel --presets es2015 --out-dir .tmp/es5/ .tmp/es2015/",
    "build-ts": "tsc",
    "clean": "rm -rf lib/ .tmp/",
    "copy-to-lib": "cp -r .tmp/es5/src/ lib/",
    "prepublish": "npm run clean && npm run build",
    "start": "node examples/index.js",
    "test": "npm run build && mocha --require intelli-espower-loader .tmp/es5/test/"
  }
}
