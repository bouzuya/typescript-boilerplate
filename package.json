{
  "name": "typescript-boilerplate",
  "description": "typescript-boilerplate",
  "version": "0.1.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/typescript-boilerplate/issues"
  },
  "devDependencies": {
    "@types/node": "6.0.38",
    "@types/power-assert": "0.0.27",
    "@types/proxyquire": "1.3.26",
    "@types/sinon": "1.16.29",
    "babel-cli": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "cpx": "1.3.2",
    "espower-babel": "^4.0.1",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^2.3.4",
    "npm-run-all": "3.0.0",
    "power-assert": "1.4.1",
    "proxyquire": "1.7.10",
    "rimraf": "2.5.4",
    "sinon": "1.17.5",
    "typescript": "2.0.0"
  },
  "homepage": "https://github.com/bouzuya/typescript-boilerplate#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/typescript-boilerplate"
  },
  "scripts": {
    "build": "npm-run-all 'build-ts' 'build-es2015' -p 'copy:*'",
    "build-es2015": "babel --presets es2015 --out-dir .tmp/es5/ .tmp/es2015/",
    "build-ts": "tsc",
    "clean": "rimraf .tmp lib",
    "copy:dts": "cpx '.tmp/es2015/src/**/*.d.ts' 'lib/'",
    "copy:js": "cpx '.tmp/es5/src/**/*.js' 'lib/'",
    "prepublish": "npm-run-all 'clean' 'build'",
    "start": "node examples/index.js",
    "test": "mocha --require intelli-espower-loader .tmp/es5/test/"
  }
}
