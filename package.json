{
  "name": "typescript-boilerplate",
  "description": "typescript-boilerplate",
  "version": "0.0.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/typescript-boilerplate/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "espower-babel": "^4.0.1",
    "mocha": "^2.3.4",
    "power-assert": "^1.2.0",
    "typescript": "^1.7.5",
    "typings": "^0.6.3"
  },
  "homepage": "https://github.com/bouzuya/typescript-boilerplate#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/typescript-boilerplate"
  },
  "scripts": {
    "build": "npm run build-ts && npm run build-es2015",
    "build-es2015": "babel --presets es2015 --out-dir lib/ .tmp/src/",
    "build-ts": "tsc",
    "clean": "rm -rf lib/ .tmp/",
    "prepublish": "typings install && npm run clean && npm run build",
    "start": "node examples/index.js",
    "test": "npm run clean && tsc && mocha --require test/espower-babel-loader.js .tmp/test/"
  }
}
